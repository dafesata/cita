<%= form_for(@historium, html:{class: "ui form historia-form"}) do |f| %>

  <% if @historium.errors.any? %>
  <div class="ui negative message">
    <div class="header">
      Tiene <%= pluralize(@historium.errors.count, "error") %>: 
      </div>
      <ul class="list">      
        <% @historium.errors.full_messages.each do |message| %>
